<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="graphicalInterface.HomePage">

   <left>
      <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" style="-fx-background-color: #00008B;" BorderPane.alignment="CENTER">
         <children>
            <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#footprintCalculator" prefHeight="70.0" prefWidth="130.0" style="-fx-background-color: transparent; -fx-border-color: #ffffff;" text="Footprint Calculator" textFill="#fffefe" />
            <Button fx:id="healthTrackerButton" layoutX="10.0" layoutY="10.0" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onMouseClicked="#healthTracker" prefHeight="70.0" prefWidth="130.0" style="-fx-background-color: transparent; -fx-border-color: #ffffff;" text="Health Tracker" textFill="#fffefe" />
            <Button fx:id="saveStatesButton" layoutX="10.0" layoutY="37.0" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onMouseClicked="#saveProfile" prefHeight="70.0" prefWidth="130.0" style="-fx-background-color: transparent; -fx-border-color: #ffffff;" text="Save Profile" textFill="#fffefe" />
            <Button fx:id="quitButton" layoutX="10.0" layoutY="64.0" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onMouseClicked="#quit" prefHeight="70.0" prefWidth="130.0" style="-fx-background-color: transparent; -fx-border-color: #ffffff;" text="Quit" textFill="#fffefe" />
         </children>
      </VBox>
   </left>
   <center>
      <VBox fx:id="homePane" alignment="TOP_CENTER" BorderPane.alignment="CENTER">
         <children>
            <Label text="Welcome to the Foot Print Calculator ">
               <font>
                  <Font size="24.0" />
               </font>
               <padding>
                  <Insets left="15.0" right="15.0" />
               </padding>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Label>
            <Label layoutX="10.0" layoutY="10.0" text="and Health Tracker Application">
               <font>
                  <Font size="24.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="30.0" />
               </VBox.margin>
            </Label>
            <Label text="In this application you can see how much CO2 you">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label layoutX="27.0" layoutY="95.0" text="produce and, track your calories, sleep and water">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
      </VBox>
   </center>
</BorderPane>
