<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="graphicalInterface.footprintCalculator.transportation.Transportation">
   <left>
      <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" style="-fx-background-color: #c8a2c8;" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="garageButton" mnemonicParsing="false" onMouseClicked="#garageMenu" prefHeight="70.0" prefWidth="130.0" style="-fx-background-color: transparent; -fx-border-color: black;" text="Garage" />
            <Button fx:id="addTransportationTripButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onMouseClicked="#addTransportationTripDisplay" prefHeight="70.0" prefWidth="130.0" style="-fx-background-color: transparent; -fx-border-color: black;" text="Add transportation trip" wrapText="true" />
            <Button fx:id="seeLastTransportationTripsButton" layoutX="10.0" layoutY="80.0" mnemonicParsing="false" onMouseClicked="#seeLastTransportationTripsDisplay" prefHeight="70.0" prefWidth="130.0" style="-fx-background-color: transparent; -fx-border-color: black;" text="See last transportation trips" wrapText="true" />
            <Button fx:id="seeTotalTransportationEmissionsButton" layoutX="10.0" layoutY="150.0" mnemonicParsing="false" onMouseClicked="#seeTotalTransportationEmissionsDisplay" prefHeight="70.0" prefWidth="130.0" style="-fx-background-color: transparent; -fx-border-color: black;" text="See total emissions from transportation " wrapText="true" />
         </children>
      </VBox>
   </left>
   <center>
      <AnchorPane fx:id="transportationDisplay" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="transportationLabel" layoutX="86.0" layoutY="132.0" text="Label" />
         </children></AnchorPane>
   </center>
</BorderPane>
