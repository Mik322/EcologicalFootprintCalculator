<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="graphicalInterface.footprintCalculator.transportation.garage.Garage">
   <left>
      <VBox style="-fx-background-color: #e3c7dd;" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="informationByCar" layoutX="10.0" layoutY="150.0" mnemonicParsing="false" onMouseClicked="#displayInformationByCar" prefHeight="70.0" prefWidth="130.0" style="-fx-background-color: transparent; -fx-border-color: black;" text="My Cars" wrapText="true" />
            <Button fx:id="addCarButton" mnemonicParsing="false" onMouseClicked="#addCarDisplay" prefHeight="70.0" prefWidth="130.0" style="-fx-background-color: transparent; -fx-border-color: black;" text="Add Car" wrapText="true" />
            <Button fx:id="editGarageButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onMouseClicked="#editGarage" prefHeight="70.0" prefWidth="130.0" style="-fx-background-color: transparent; -fx-border-color: black;" text="Edit Garage" wrapText="true" />
            <Button fx:id="display_information" layoutX="10.0" layoutY="295.0" mnemonicParsing="false" onMouseClicked="#information" prefHeight="70.0" prefWidth="130.0" style="-fx-background-color: transparent; -fx-border-color: black;" text="Informations" wrapText="true" />
         </children>
      </VBox>
   </left>
   <center>
      <AnchorPane fx:id="garageDisplay" prefHeight="630.0" prefWidth="381.0" BorderPane.alignment="CENTER">
         <children>
            <Label layoutX="81.0" layoutY="265.0" text="Welcome to your garage!">
               <font>
                  <Font size="21.0" />
               </font>
            </Label>
         </children></AnchorPane>
   </center>
</BorderPane>
