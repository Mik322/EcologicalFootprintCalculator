<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane VBox.vgrow="ALWAYS" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="graphicalInterface.healthTracker.addChange.changeBodyParameters.ChangeBodyParameters">
   <center>
      <VBox fx:id="changeBodyParametersDisplay" alignment="CENTER" BorderPane.alignment="CENTER" />
   </center>
   <left>
      <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" style="-fx-background-color: white; -fx-border-color: #20AF00#20AF00;" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="changeHeightButton" mnemonicParsing="false" onMouseClicked="#changeHeightDisplay" prefHeight="70.0" prefWidth="130.0" style="-fx-background-color: transparent;" text="Change Height" textAlignment="CENTER" textFill="#20af00" wrapText="true">
               <font>
                  <Font name="Be Vietnam Regular" size="13.0" />
               </font></Button>
            <Button fx:id="changeWeightButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onMouseClicked="#changeWeightDisplay" prefHeight="70.0" prefWidth="130.0" style="-fx-background-color: transparent;" text="Change Weight" textAlignment="CENTER" textFill="#20af00" wrapText="true">
               <font>
                  <Font name="Be Vietnam Regular" size="13.0" />
               </font></Button>
            <Button fx:id="changeAgeButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onMouseClicked="#changeAgeDisplay" prefHeight="70.0" prefWidth="130.0" style="-fx-background-color: transparent;" text="Change Age" textAlignment="CENTER" textFill="#20af00" wrapText="true">
               <font>
                  <Font name="Be Vietnam Regular" size="13.0" />
               </font></Button>
            <Button fx:id="changeLifestyleButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onMouseClicked="#changeLifestyleDisplay" prefHeight="70.0" prefWidth="130.0" style="-fx-background-color: transparent;" text="Change Lifestyle" textAlignment="CENTER" textFill="#20af00" wrapText="true">
               <font>
                  <Font name="Be Vietnam Regular" size="13.0" />
               </font></Button>
         </children>
      </VBox>
   </left>
</BorderPane>
