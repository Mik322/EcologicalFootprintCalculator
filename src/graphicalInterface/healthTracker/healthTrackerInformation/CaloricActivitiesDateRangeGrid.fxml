            <Label alignment="CENTER" contentDisplay="CENTER" prefWidth="225.0" text="Activity" />
            <Label alignment="CENTER" contentDisplay="CENTER" prefWidth="187.0" text="Quantity/Time" GridPane.columnIndex="1" />
            <Label alignment="CENTER" contentDisplay="CENTER" prefWidth="192.0" text="Date" GridPane.columnIndex="2" />
            <Label alignment="CENTER" contentDisplay="CENTER" prefWidth="225.0" text="Activity Type" GridPane.columnIndex="3" />
         </children>
          <VBox.margin>
              <Insets bottom="40.0" />
          </VBox.margin>
      </GridPane>
      <ScrollPane fx:id="scrollPane" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="300.0" prefWidth="600.0">
          <padding>
              <Insets top="20.0" />
          </padding>
         <content>
            <VBox fx:id="elements" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" />
         </content>
      </ScrollPane>
